{
    "version": 2,
    "builds": [
        {
            "src": "apps/web/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "apps/web/$1"
        }
    ],
    "installCommand": "npm install",
    "buildCommand": "cd apps/web && npm run build",
    "outputDirectory": "apps/web/dist"
}